{
  "name": "replay-vercel",
  "version": "1.0.0",
  "description": "Full-stack reflection and journaling application on Vercel",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "build": "npm run install:clean && cd client && npm run build",
    "build:client": "cd client && npm run build",
    "install:clean": "cd client && rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "deploy": "vercel --prod",
    "push:generate-sw-config": "node scripts/generate-firebase-sw-config.mjs",
    "push:setup-icons": "python3 scripts/generate-apns-iconset.py",
    "push:manifest": "node scripts/generate-apns-manifest.mjs",
    "push:sign": "scripts/sign-apns-push-package.sh",
    "push:archive": "scripts/archive-apns-push-package.sh",
    "push:validate": "node scripts/validate-apns-package.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.55.0",
    "busboy": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "replicate": "^1.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": "22.x"
  }
}
