{
  "name": "replay-vercel",
  "version": "1.0.0",
  "description": "Full-stack reflection and journaling application on Vercel",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"vercel dev\"",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "start": "vercel dev",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "@google/generative-ai": "^0.24.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "replicate": "^1.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": "18.x"
  }
}